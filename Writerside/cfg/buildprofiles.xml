               xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">

    <variables>
        <primary-color>#3c9566</primary-color>
        <custom-favicons>profiles-favicon_test.svg</custom-favicons>
        <header-logo>profiles-logo_test_green.svg</header-logo>
        <product-web-url>https://qa.gaoyang.wiki/manuscript/</product-web-url>
